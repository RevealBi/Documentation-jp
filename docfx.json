{
  "pdf": {
    "content": [
      {
        "files": [
          "jp/developer/**.yml"
        ],
        "exclude": [
          "**/toc.yml",
          "**/toc.md"
        ]
      },
      {
        "files": [
          "jp/developer/**.md",
          "jp/developer/**/toc.yml",
          "toc.yml",
          "*.md",
          "pdf/*"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site_pdf/**",
          "**/toc.yml",
          "**/toc.md"
        ]
      },
      {
        "files": "pdf/toc.yml"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/images/**"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**",
          "_site_pdf/**"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site_pdf/**"
        ]
      }
    ],
    "wkhtmltopdf": {
      "additionalArguments": "--enable-local-file-access --encoding utf-8"
    },
    "dest": "_site_pdf"
},
  "build": {
    "content": [
      {
        "files": [
          "jp/**.md",
          "jp/**/toc.yml"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "jp/developer/archived-docs/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"

        ]
      },
      {
        "files": [
          "jp/index.html",
          "jp/index.css"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "markdownEngineName": "markdig",
    "template": [
      "templates/reveal"
    ],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true,
    "globalMetadata": {
      "_appTitle": "Reveal",
      "_navBarTitle": "Reveal",
      "_isLangEn": false,
      "_isLangJa": true,
      "_navBarTitleHref": "https://www.revealbi.io/",
      "_gitContribute": {
        "repo": "https://github.com/RevealBi/Documentation-jp",
        "branch": "master"
      }
    }
  }
}